{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "inlineSourceMap": true,
        "inlineSources": true,
        "noImplicitAny": true,
        "strict": true,
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "declaration": true,
        "outDir": "js/es5"
    },
    "include": [
        "ts/**/*.ts"
    ]
}